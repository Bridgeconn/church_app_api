<p id="notice"><%= notice %></p>

<h1>Church Apps</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @church_apps.each do |church_app| %>
      <tr>
        <td><%#= link_to 'Show', user_church_app_path(current_user.id) %></td>
        <td><%#= link_to 'Edit', edit_user_church_app_path(church_app) %></td>
        <td><%#= link_to 'Destroy', user_church_app_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Church App', new_admin_user_user_church_app_path(current_user) %>
